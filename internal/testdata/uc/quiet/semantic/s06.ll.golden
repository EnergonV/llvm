target datalayout = "e-m:e-i64:64-f80:128-n8:16:32:64-S128"
target triple = "x86_64-unknown-linux-gnu"
define i32 @main() {
0:
	%x = alloca i8
	%y = alloca i32
	%1 = load i32, i32* %y
	%2 = trunc i32 %1 to i8
	store i8 %2, i8* %x
	%3 = load i8, i8* %x
	%4 = sext i8 %3 to i32
	store i32 %4, i32* %y
	ret i32 0
}
